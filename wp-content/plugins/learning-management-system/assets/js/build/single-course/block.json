{
  "$schema": "https://schemas.wp.org/block-json/1.1",
  "apiVersion": 3,
  "name": "masteriyo/single-course",
  "version": "1.0.0",
  "title": "Single Course",
  "category": "masteriyo",
  "description": "",
  "keywords": [
    "Single Course"
  ],
  "textdomain": "learning-management-system",
  "providesContext": {
    "masteriyo/course_id": "courseId"
  },
  "supports": {
    "className": false,
    "customClassName": false,
    "reusable": false
  },
  "attributes": {
    "clientId": {
      "type": "string"
    },
    "courseId": {
      "type": "number"
    },
    "margin": {
      "type": "string",
      "default": "auto"
    },
    "padding": {
      "type": "object",
      "default": {
        "border": 1,
        "padding": {
          "desktop": {
            "top": 0,
            "right": 0,
            "bottom": 0,
            "left": 0,
            "unit": "px"
          },
          "tablet": {
            "top": 0,
            "right": 0,
            "bottom": 0,
            "left": 0,
            "unit": "px"
          },
          "mobile": {
            "top": 0,
            "right": 0,
            "bottom": 0,
            "left": 0,
            "unit": "px"
          }
        }
      }
    },
    "blockCSS": {
      "type": "string"
    }
  },
  "style": "masteriyo-blocks",
  "editorScript": "masteriyo-blocks",
  "editorStyle": "masteriyo-blocks-editor"
}
